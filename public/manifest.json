{

  "manifest_version": 2,
  "name": "FlosStartseite",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",
  "content_scripts": [],


  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": [
    "tabs",
    "browserSettings",
    "history",
    "<all_urls>"
  ]

}